pipeline {
    agent any
    tools {
        maven 'maven3'
    }


    stages {
        stage('Git Checkout') {
            steps {
                git 'https://github.com/ajayaj3008/devopsproject02082025.git'
            }
        }
        stage('Maven Compile') {
            steps {
                echo 'Maven Compile Started'
                sh 'mvn compile'
            }
        }
        stage('Maven Test') {
            steps {
                echo 'Maven Test Started'
                sh 'mvn test'
            }
        }
        
        
    }
}
